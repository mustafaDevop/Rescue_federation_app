{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "apk": {
        "android": {
          "buildType": "apk"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }
}